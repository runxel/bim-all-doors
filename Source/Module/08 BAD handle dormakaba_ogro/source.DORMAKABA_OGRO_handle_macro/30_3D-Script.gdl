"LoD_handles":
	sts=0
    IF dk_LOD < 101 THEN GOTO "LoD100H"
	IF dk_LOD > 100 AND dk_LOD < 201 THEN GOTO "LoD200H"
	IF dk_LOD > 200 AND dk_LOD < 301 THEN GOTO "LoD300"
	IF dk_LOD > 300 AND dk_LOD < 351 THEN GOTO "LoD350"
	IF dk_LOD > 350 AND dk_LOD < 401 THEN GOTO "LoD400"
    IF dk_LOD > 400 THEN GOTO "LoD500"
    RETURN

