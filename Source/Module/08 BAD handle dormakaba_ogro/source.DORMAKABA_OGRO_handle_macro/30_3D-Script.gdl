"Model-ZL 65xx Tchoban":
	! obere Rosette
	! r = 0.063/2
	! t  = 0.061
	! FOR idx1=1 TO 3
	! 	w = idx1*120
	! 	PUT COS(w- 30)*t, SIN(w- 30)*t, 900+smth
	! 	PUT COS(w+ 90)*r, SIN(w+ 90)*r, smth
	! 	PUT COS(w+210)*r, SIN(w+210)*r, 3000+smth
	! 	NEXT idx1
	r = 0.038
	t  = 0.0692
	FOR idx1=1 TO 3
		w = 180+idx1*120
		PUT COS(w+ 30)*r, SIN(w+ 30)*r, smth
		PUT COS(w- 90)*t, SIN(w- 90)*t, 900+smth
		PUT COS(w+120)*r, SIN(w+120)*r, 3000+smth
		NEXT idx1
	idx1 = 3 * 3
	GOSUB "Prismenausgabe"

	! untere Rosette
	ADDy -bs_case_distance
	r = 0.038
	t  = 0.0692
	FOR idx1=1 TO 3
		w = idx1*120
		PUT COS(w+ 30)*r, SIN(w+ 30)*r, smth
		PUT COS(w- 90)*t, SIN(w- 90)*t, 900+smth
		PUT COS(w+120)*r, SIN(w+120)*r, 3000+smth
		NEXT idx1
	! FOR idx1=1 TO 3
	! 	w = 180 + idx1*120
	! 	PUT COS(w+ 90)*r, SIN(w+ 90)*r+s, smth
	! 	PUT COS(w- 30)*t, SIN(w- 30)*t+s, 900+smth
	! 	PUT COS(w+210)*r, SIN(w+210)*r+s, 3000+smth
	! 	NEXT idx1
	PUT COS(210)*r, SIN(210)*r+s, -1
	idx1 = 3 * 3 +1
	! IF putCLSR # "" AND putCLSR # "clsr-WCS" AND putCLSR # "clsr-WCK" THEN  GOSUB putCLSR
	IF putCLSR # "" THEN  GOSUB putCLSR
	idx1 = idx1 + idx2
	GOSUB "Prismenausgabe"
	IF putCLSR # "" THEN  GOSUB "Schloss"
	! IF putCLSR # "" AND putCLSR # "clsr-WCS" AND putCLSR # "clsr-WCK" THEN  GOSUB "Schloss"
	DEL 1
	RETURN

