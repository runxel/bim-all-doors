"LoD100H":
	sts=2
    RETURN

! »»» Reduzierte Darstellung (Linie)

"LoD200H":
	sts=2
   	RETURN

"LoD300":
	sts=1
    LIN_ 0, 0,0, 0,-y,0
    LIN_ 0,-y,0, x,-y,0
    RETURN

! »»» Komplexe Darstellung

"LoD350":
	sts=1
	ADDz -d/2
	PRISM 6,d,
		-d/2,0,
		-d/2,-y,
		x,-y,
		x,-y+d,
		d/2,-y+d,
		d/2,0
	DEL 1
    RETURN

