"repetitive":
	ROTX -90
	ADDZ -h1
	GROUP "ogro1"
	HPRISM_	matG, matG, matG, matG,
			5, 0.0035, 45, 0.001, 0,
			0, r, 78,
			vers, r, 78,
			vers, -r, 1078,
			0, -r, 78,
			0, r, 1078
	ENDGROUP
	GROUP "ogro2"
	MULZ -1
	HPRISM_	matG, matG, matG, matG,
			5, 0.0035, 45, 0.001, 0,
			0, r, 78,
			vers, r, 78,
			vers, -r, 1078,
			0, -r, 78,
			0, r, 1078
	DEL 1
	ENDGROUP
	GROUP "ogro3"
	CYLIND h1, r
	ADD vers, 0, -h2
	CYLIND h2, r
	DEL 1
	ENDGROUP
	result_1=ADDGROUP("ogro1", "ogro2")
	result_2=ADDGROUP("ogro3", result_1)
	PLACEGROUP result_2
	KILLGROUP "ogro1"
	KILLGROUP "ogro2"
	KILLGROUP "ogro3"
	DEL 2
	RETURN


