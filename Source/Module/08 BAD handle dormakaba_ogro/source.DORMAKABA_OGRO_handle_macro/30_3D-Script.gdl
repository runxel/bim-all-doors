"Prismenausgabe":
	IF ra > 0 THEN
		HPRISM_	matP, matP, matP, matP,
			idx1, plateD, 45, ra, 1,
			GET(idx1*3)
		ELSE
		PRISM_	idx1, plateD, GET(idx1*3)
		ENDIF
	RETURN

"Schloss":
	idx2 = 0
	MATERIAL dk_matSchwarz
	GOSUB putCLSR
	PRISM_	idx2, plateD*0.75, GET(idx2*3)

	! WC-Schloss Schild
	IF putCLSR = "clsr-WCS" THEN
		ADDz plateD
		PRISM_	2, plateD*0.2,
			0,0, 900+smth,
			0.0082,360, 4000+smth2
		DEL 1
		ENDIF

	! WC-Schloss Knebel
	IF putCLSR = "clsr-WCK" THEN
		MATERIAL matP
		ADDz plateD
		PRISM_	2, 0.008,
			0,0, 900+smth,
			0.012,360, 4000+smth
		ADDz 0.008
		r = 0.0025
		smth=1+2+8+64
		HPRISM_	matP, matP, matP, matP,
			9, 0.015, 0, r, 1,
			 0.005  ,-0.022+r, 1+8,
			 0.005  , 0.022-r, smth,
			 0.005-r, 0.022  , 1000+smth,
			-0.005+r, 0.022  , smth,
			-0.005  , 0.022-r, 1000+smth,
			-0.005  ,-0.022+r, smth,
			-0.005+r,-0.022  , 1000+smth,
			 0.005-r,-0.022  , smth,
			 0.005  ,-0.022+r, 1000+smth
		DEL 2
		ENDIF
	RETURN

