"devshow":
	UI_STYLE 1,2
	UI_OUTFIELD "[HANDLE v" + str("%^.2",version) + "]"  , 60,0,100,15, 4
	UI_STYLE 0,0

	i = -1 : s = 20
	i = i+1 :	UI_INFIELD{3} "bs_handle_model",	475,i*s, 45,19  UI_TOOLTIP "bs_handle_model"
				UI_INFIELD{3} "bs_handle_model2",	525,i*s, 45,19  UI_TOOLTIP "bs_handle_model2"
				UI_INFIELD{3} "bs_handle_model3",	575,i*s, 45,19  UI_TOOLTIP "bs_handle_model3"
	i = i+1 :	UI_INFIELD{3} "bs_doorplate1",	475,i*s, 45,19  UI_TOOLTIP "bs_doorplate1"
				UI_INFIELD{3} "bs_doorplate2",	525,i*s, 45,19  UI_TOOLTIP "bs_doorplate2"
				UI_INFIELD{3} "bs_doorplate3",	575,i*s, 45,19  UI_TOOLTIP "bs_doorplate3"
	i = i+1 :	UI_INFIELD{3} "bs_access_control_system",	475,i*s, 145,19  UI_TOOLTIP "bs_access_control_system"
	i = i+1 :	UI_INFIELD{3} "bs_control_side",	        475,i*s, 95,19  UI_TOOLTIP "bs_control_side"
				ui_outfield bs_acs_function, 575,i*s, 45,15  UI_TOOLTIP "bs_acs_function"
	i = i+1 :	ui_outfield bs_acs_handles,		 475,i*s, 45,15  UI_TOOLTIP "bs_acs_handles"
				ui_outfield bs_door_electrified, 525,i*s, 45,15  UI_TOOLTIP "bs_door_electrified"
	RETURN

"devSELECTION":
	FOR i= 1 TO VARDIM1(UIpicIdxArray)
		ui_outfield UIpicIdxArray[i],   0,260+i*15,140,15
		ui_outfield UItextArray[i]  , 150,260+i*15,140,15
		ui_outfield UIvaluesArray[i], 300,260+i*15,140,15
		NEXT i
	RETURN

"error2tooltip":
	IF errorI > 0 THEN txt = errorQ[1] ELSE txt = ""
	FOR i=2 TO errorI : txt = txt + errorQ[i] + "  " : NEXT i
	IF STRLEN(txt) > 80 THEN txt = STRSUB(txt,1,77) + "..."
	RETURN

