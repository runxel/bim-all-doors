"SELECT_bs_doorplate1":
	DIM  UIpicIdxArray[], UItextArray[], UIvaluesArray[]
	sts = (dk_LoI > 4)
	! Griff nicht spezifiziert oder aus: Dann Standardwerte
	! IF selectedBS<1 OR gs_handle_type = "-" THEN : sts=0 : ELSE : IF bs_handle_model="-" THEN sts=0 : ENDIF
	UIpicIdxArray = UIplateModelPic : UItextArray = UIplateModelText : UIvaluesArray = UIplateModelVal
	IF NOT(sts) THEN GOSUB "VALUES_bs_doorplate1"  ! LOI fragt nicht alles ab, dann alles Standardwerte als Textliste
	RETURN

! »»» Standardschilder

"VALUES_bs_doorplate1":
"VALUES_bs_doorplate2":
"VALUES_bs_doorplate3":
	FOR j = 1 TO VARDIM1(UIvaluesArray)
		IF UIvaluesArray[j] = "650x + 6612" THEN UItextArray[j] = "Rosetten"
		IF UIvaluesArray[j] = "650x + 7122" THEN UItextArray[j] = "Rosetten"
		IF UIvaluesArray[j] = "650x + 7123" THEN UItextArray[j] = "Rosetten (Metallschild)"
		IF UIvaluesArray[j] = "650x" THEN UItextArray[j] = "Rosette (Griffloch)"
		IF UIvaluesArray[j] = "6612" THEN UItextArray[j] = "Rosette (Schloß)"
		IF UIvaluesArray[j] = "7122" THEN UItextArray[j] = "Rosette WC (Schloß)"
		IF UIvaluesArray[j] = "7123" THEN UItextArray[j] = "Rosette WC (Schloß/Metallschild)"
		IF UIvaluesArray[j] = "705x K" THEN UItextArray[j] = "Kurzschild rund"
		IF UIvaluesArray[j] = "706x K" THEN UItextArray[j] = "Kurzschild eckig"
		IF UIvaluesArray[j] = "702x L" THEN UItextArray[j] = "Langschild rund"
		IF UIvaluesArray[j] = "701x L" THEN UItextArray[j] = "Langschild eckig"
		IF UIvaluesArray[j] = "7601" THEN UItextArray[j] = "Breitschild"
		IF UIvaluesArray[j] = "662x + 6679" THEN UItextArray[j] = "Rosetten rund"
		IF UIvaluesArray[j] = "661x + 6676" THEN UItextArray[j] = "Rosetten eckig"
		IF UIvaluesArray[j] = "662x" THEN UItextArray[j] = "Rosette rund (Griffloch)"
		IF UIvaluesArray[j] = "661x" THEN UItextArray[j] = "Rosette eckig (Griffloch)"
		IF UIvaluesArray[j] = "6679" THEN UItextArray[j] = "Rosette rund (Schloß)"
		IF UIvaluesArray[j] = "6676" THEN UItextArray[j] = "Rosette eckig (Schloß)"
		IF UIvaluesArray[j] = "ZL 65xx Tchoban" THEN UItextArray[j] = "Rosetten"
		IF UIvaluesArray[j] = "ZL 65xx Forstner" THEN UItextArray[j] = "Rosetten"
		NEXT j
	RETURN


! »»» Verfügbare Lochungen

"SELECT_bs_closure1":
	j = 0 : DIM  UIpicIdxArray[], UItextArray[], UIvaluesArray[]
	j=1 : UIpicIdxArray[j] = 1 : UItextArray[j] = "Blindschild" : UIvaluesArray[j] = "-"

	! »»» Lochung vorgesehen?
	sts=-1
	IF selectedPlateBS<1 OR bs_doorplate1="-" THEN
		ELSE
		FOR m=1 TO VARDIM2(lochung)
			IF lochung[selectedPlateBS][m] THEN sts=ABS(sts)
			NEXT m
		ENDIF

	! »»» Griffspezifische Auswahl?
	IF sts>0 THEN
		IF selectedBS<1 THEN
			sts=0
			ELSE
			IF bs_handle_model="-" THEN sts=0
			ENDIF
		ENDIF

	IF sts>0 THEN
		IF lochung[selectedBS][dki_Profilzylinder] THEN : j=j+1 : UIpicIdxArray[j] = 2 : UItextArray[j] = "Profilzylinder" : UIvaluesArray[j] = "PZ" : ENDIF
		IF lochung[selectedBS][dki_Rundzylinder]   THEN : j=j+1 : UIpicIdxArray[j] = 3 : UItextArray[j] = "Rundzylinder" :   UIvaluesArray[j] = "RZ" : ENDIF
		IF lochung[selectedBS][dki_Ovalzylinder]   THEN : j=j+1 : UIpicIdxArray[j] = 6 : UItextArray[j] = "Ovallochung" :	UIvaluesArray[j] = "OZ" : ENDIF
		IF doorTYPE # 1 THEN
			IF lochung[selectedBS][dki_Buntbart]   THEN : j=j+1 : UIpicIdxArray[j] = 4 : UItextArray[j] = "Buntbart" :	   UIvaluesArray[j] = "BB" : ENDIF
			IF lochung[selectedBS][dki_LochungCH]  THEN : j=j+1 : UIpicIdxArray[j] = 5 : UItextArray[j] = "Kegellochung" :	 UIvaluesArray[j] = "CH" : ENDIF
			IF lochung[selectedBS][dki_WC]		   THEN : j=j+1 : UIpicIdxArray[j] = 7 : UItextArray[j] = "WC-Schaubild" :   UIvaluesArray[j] = "WCS" : ENDIF
			IF lochung[selectedBS][dki_WC]		   THEN : j=j+1 : UIpicIdxArray[j] = 8 : UItextArray[j] = "WC-Knebel" :	  UIvaluesArray[j] = "WCK" : ENDIF
			ENDIF
		ELSE
		IF sts=0 THEN GOSUB "VALUES_bs_closure1" ELSE UItextArray[j] = "Aus"
		ENDIF

	RETURN

