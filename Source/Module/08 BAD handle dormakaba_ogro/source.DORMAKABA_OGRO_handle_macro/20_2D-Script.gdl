
IF NOT( BITTEST(macro_runtype,4) AND dk_visibleLeaf) OR (noHandle AND noLockPlate) THEN END

dk_handle_fill = 64   ! Massive Schraffur
drawMODELL = ""       ! Makroname
rothandle  = 0        ! Drehung Griff in°
d = 0 : x = 0 : y = 0 !Abmessungen Griff

PEN dk_cont_pens[dka_handle_penC2]
FILL dk_handle_fill

! =============================================================================
! Bandseite
! =============================================================================

IF BITTEST(macro_runtype,0) THEN
    rothandle = 90*(bs_handle_orientation="So") - 90*(bs_handle_orientation="Su")
    IF selectedPlateBS > 0 AND NOT(noPlate) THEN    ! Abdeckung
        IF bs_doorplate1 = "-"  OR dk_LOD < 101 THEN
            ! Kein Schild oder projiziert
            plateD = 0
            ELSE
            plateD = abmessung[selectedPlateBS][dki_D]
            plateX = abmessung[selectedPlateBS][dki_Breite]
            plateY = abmessung[selectedPlateBS][dki_Tiefe]
            plateS = abmessung[selectedPlateBS][dki_VersatzX]
            GOSUB "drawPLATE"
            ENDIF
        ELSE
        plateD = 0
        ENDIF
    IF gs_handle_type  # "-" AND NOT(noHandle) THEN    ! Griff
        ADD2 0,-plateD
        IF bs_handle_model = "-" OR selectedBS<1 THEN  ! Standardgriff (undefiniert)
	        i = (MAX(0,STRSTR("DRKFKD",gs_handle_type)-1)) + (doorTYPE=1)
	        GOSUB "undefinedTypes"
	        selectedBS = j : bs_handle_model = txt
            ENDIF
        handleD = abmessung[selectedBS][dki_D]
        handleX = abmessung[selectedBS][dki_Breite] - handleD/2
        handleY = abmessung[selectedBS][dki_Tiefe]
        GOSUB "LoD_handles"
        IF NOT(sts) THEN GOSUB "Model-"+bs_handle_model
        DEL 1
        ENDIF
    ENDIF
    
! =============================================================================
! Bandgegenseite
! =============================================================================

IF BITTEST(macro_runtype,1) THEN
    rothandle = 90*(bs_handle_orientation2="So") - 90*(bs_handle_orientation2="Su")
    IF selectedPlateBGS > 0 AND NOT(noPlate) THEN    ! Abdeckung
        IF bs_doorplate2 = "-"  OR dk_LOD < 101 THEN
            ! Kein Schild oder projiziert
            plateD = 0
            ELSE
            plateD = abmessung[selectedPlateBGS][dki_D]
            plateX = abmessung[selectedPlateBGS][dki_Breite]
            plateY = abmessung[selectedPlateBGS][dki_Tiefe]
            plateS = abmessung[selectedPlateBGS][dki_VersatzX]
            GOSUB "drawPLATE"
            ENDIF
        ELSE
        plateD = 0
        ENDIF
	IF gs_handle_type2 # "-" AND NOT(noHandle) THEN    ! Griff
	    ADD2 0,-plateD
        IF bs_handle_model2 = "-" OR selectedBGS<1 THEN  ! Standardgriff (undefiniert)
	        i = (MAX(0,STRSTR("DRKFKD",gs_handle_type2)-1)) + (doorTYPE=1)
	        GOSUB "undefinedTypes"
	        selectedBGS = j : bs_handle_model2 = txt
            ENDIF
        handleD = abmessung[selectedBGS][dki_D]
        handleX = abmessung[selectedBGS][dki_Breite] - handleD/2
        handleY = abmessung[selectedBGS][dki_Tiefe]
        GOSUB "LoD_handles"
        IF NOT(sts) THEN GOSUB "Model-"+bs_handle_model2
	    DEL 1
	    ENDIF
    ENDIF

! =============================================================================
! Standflügel
! =============================================================================

IF BITTEST(macro_runtype,2) THEN
    rothandle = 90*(bs_handle_orientation3="So") - 90*(bs_handle_orientation3="Su")
    IF selectedPlateSF > 0 AND NOT(noPlate) THEN    ! Abdeckung
        IF bs_doorplate3 = "-"  OR dk_LOD < 101 THEN
            ! Kein Schild oder projiziert
            plateD = 0
            ELSE
            plateD = abmessung[selectedPlateSF][dki_D]
            plateX = abmessung[selectedPlateSF][dki_Breite]
            plateY = abmessung[selectedPlateSF][dki_Tiefe]
            plateS = abmessung[selectedPlateSF][dki_VersatzX]
            GOSUB "drawPLATE"
            ENDIF
        ELSE
        plateD = 0
        ENDIF
	IF gs_handle_type3 # "-" AND NOT(noHandle) THEN    ! Griff
	    ADD2 0,-plateD
        IF bs_handle_model3 = "-" OR selectedSF<1 THEN  ! Standardgriff (undefiniert)
	        i = (MAX(0,STRSTR("DRKFKD",gs_handle_type3)-1)) + (doorTYPE=1)
	        GOSUB "undefinedTypes"
	        selectedSF = j : bs_handle_model3 = txt
            ENDIF
        handleD = abmessung[selectedSF][dki_D]
        handleX = abmessung[selectedSF][dki_Breite] - handleD/2
        handleY = abmessung[selectedSF][dki_Tiefe]
        GOSUB "LoD_handles"
        IF NOT(sts) THEN GOSUB "Model-"+bs_handle_model3
	    DEL 1
	    ENDIF
    ENDIF

! ------------------------------------------------------------------------- [ ENDE ]

END htspt



! =============================================================================
! =============================================================================
!
! SUBROUTINES
!
! =============================================================================
! =============================================================================

"drawPLATE":
    ! htspt=htspt+1 : HOTSPOT2 0,0, htspt
    POLY2_B 5, 1+2, dk_fill_pens[dka_handle_penF], dk_fill_pens[dka_handle_penF], 
        -plateS, 0,  1,
        -plateS, -plateD,  1,
        -plateS+plateX, -plateD,  1,
        -plateS+plateX, 0,  1,
        -plateS, 0, -1
    RETURN

! ------------------------------------------------------------------------ [ MODELLE 2D ]

"LoD_handles":
    sts  = 0
    IF dk_LOD < 101 THEN GOTO "LoD100H"  ! Keine Ausgabe
    IF dk_LOD > 100 AND dk_LOD < 201 THEN GOTO  "LoD200H"
    IF dk_LOD > 200 AND dk_LOD < 301 THEN GOTO "LoD300H"
    htspt=htspt+1 : HOTSPOT2 0,plateD, htspt
    GOSUB "Fangpunkte"
    IF dk_LOD > 300 AND dk_LOD < 351 THEN GOTO "LoD350H"
    IF dk_LOD > 350 AND dk_LOD < 401 THEN GOTO "LoD400H"
    IF dk_LOD > 400 THEN GOTO "LoD400H"
    RETURN

! »»» Einfache Darstellung

"LoD100H":
    sts = 1
    RETURN

! »»» Reduzierte Darstellung

"LoD200H":
    IF rothandle THEN
        LINE2 0,0, 0,-handleY
        ELSE
        POLY2 3,1, 0,0, 0,-handleY, handleX,-handleY
        ENDIF
    sts = 1
    RETURN

! »»» Komplexe Darstellung

"Fangpunkte":
    ! Fangpunkte
    IF rothandle THEN
        ! senkrecht
        htspt=htspt+1 : HOTSPOT2 0,-handleY, htspt
        ELSE
        ! waagrecht
        htspt=htspt+1 : HOTSPOT2 -handleD/2,-handleY, htspt
        htspt=htspt+1 : HOTSPOT2 handleX,-handleY, htspt
        ENDIF
    RETURN

"LoD300H":
    s = MAX(x, d/2)
    IF rothandle THEN
        POLY2_B 5, 1+2, dk_fill_pens[dka_handle_penF], dk_fill_pens[dka_handle_penF], 
            -handleD/2,  0,  1,
             handleD/2,  0,  1,
             handleD/2, -handleY,  1,
            -handleD/2, -handleY,  1,
            -handleD/2,  0, -1
        ELSE
        POLY2_B 7, 1+2, dk_fill_pens[dka_handle_penF], dk_fill_pens[dka_handle_penF], 
            -handleD/2,0,1,
            -handleD/2, -handleY, 1,
               handleX, -handleY, 1,
               handleX, -handleY+handleD, 1,
             handleD/2, -handleY+handleD, 1,
             handleD/2,  0, 1,
            -handleD/2,  0,-1
        ENDIF
    sts = 1
    RETURN


"LoD350H":
"LoD400H":
    IF rothandle THEN
        ! Gedrehte Ausrichtung nur als 3D-Projektion
        sts = 1
        GOTO "LoD500H"
        ENDIF
    RETURN

! »»» Komplexe Darstellung

"LoD500H":
    PROJECT2{4} 3,270,  ! projection_code, angle,
        0, 1,           ! useTransparency, statusParts,
        0,              ! numCutplanes,
        3+32,1+2+4+8,   ! method1, parts1,
        dk_handle_fill, ! cutFillIndex1,
        dk_fill_pens[dka_handle_penF],     ! cutFillFgPen1,  
        dk_fill_pens[dka_handle_penF],     ! cutFillBgPen1,
        0,0, 0,                            ! cutFillOrigoX1, cutFillOrigoY1, cutFillDirection1,
        dk_cont_pens[dka_handle_penC2], 1, ! cutLinePen1, cutLineType1,
        dk_handle_fill,                    ! projectedFillIndex1,
        dk_fill_pens[dka_handle_penF],     ! projectedFillFgPen1,  
        dk_fill_pens[dka_handle_penF],     ! projectedFillBgPen1,
        0,0, 0,                            ! projectedFillOrigoX1, projectedFillOrigoY1, projectedFillDirection1,
        dk_cont_pens[dka_handle_penC2], 1, ! projectedLinePen1, projectedLineType1
        PARAMETERS macro_runtype = BITSET(macro_runtype,5), dk_LOD = 500
    sts = 1
    RETURN

! »»» Ausgabe über Makro

"Freeform":
    IF drawMODELL # "" THEN ! Freiformmodelle - Grundrissdarstellung über Makros
        ! CALL drawMODELL PARAMETERS htspt = htspt, dk_LOD = dk_LOD, 
        !     x = x, y = y, d = d,
        !     gs_fill_type = dk_handle_fill,
        !     gs_cont_pen = dk_cont_pens[dka_handle_penC2],
        !     gs_fill_pen = dk_fill_pens[dka_handle_penF], 
        !     gs_back_pen = dk_fill_pens[dka_handle_penF]
        GOTO "LoD500H"
        ENDIF
    RETURN


