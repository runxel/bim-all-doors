! ------------------------------------------------------------- Plausibilitätschecks
"checkUsableClearance":

	IF  ((bs_UsableClearance - bs_quality_UsableClearance) < -tlr AND bs_door_leafs > 0 AND BITTEST(dk_UCrelation, 5)) OR \
		((bs_UsableClearance - bs_quality_TotalClearance)  < -tlr AND bs_door_leafs = 2 AND NOT(BITTEST(dk_UCrelation, 5))) OR \
		((ac_egress_width - bs_quality_TotalClearance)     < -tlr AND bs_door_leafs = 0) THEN
		errorI = errorI+1 : errorQ[errorI] = "Erforderliche lichte Durchgangsbreite zu gering." : errorT[errorI] = "... allgemeiner Fehler"
		ENDIF

	IF  ((bs_UsableHeight - bs_quality_UsableHeight)  < -tlr AND bs_door_leafs > 0) OR \
		((ac_egress_height - bs_quality_UsableHeight) < -tlr AND bs_door_leafs = 0) THEN
		errorI = errorI+1 : errorQ[errorI] = "Erforderliche lichte Durchgangshöhe zu gering." : errorT[errorI] = "... allgemeiner Fehler"
		ENDIF

	RETURN


