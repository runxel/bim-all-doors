! ------------------------------------------------------------- INFOSEITE / WARNUNGEN [999]

999:

	! #### LOI 1+ - Warnmeldungsseite

	errorI = 0

	!»» Fehler aus Hauptobjekt ermitteln
	GOSUB "checkUsableClearance"

	! Spiegelung der Bandseite erzeugt Fehler in Raumzuordnung. BS/BGS nicht mehr klar zu ermitteln.
	IF  WIDO_REVEAL_SIDE THEN
		errorI = errorI+1 : errorQ[errorI] = uiTXT[10] : errorT[errorI] = uiTXT[11]
		ENDIF

	!»» Fehlermeldungen aus Makros abfragen
	GOSUB "checkErrorMessages"

	!»» Seite anlegen
	IF errorI < 1 THEN btmp = "uiTab_Info.png" ELSE btmp = "dk_attention 16x16.png"
	UIpage = UIpage + 1 : UI_PAGE UIpage, UIpage_masterparent, uiTXT[2], btmp

	GOSUB 9950

	UI_OUTFIELD uiTXT[12], 0,16 , 444,28, 2+0

	IF errorI<1 THEN
		UI_OUTFIELD uiTXT[13], 0,60 , 444,28, 2+0							
		ENDIF

	IF errorI<9 THEN
		s=27 : UI_STYLE 0,0
		ELSE
		s=24 : UI_STYLE 1,0
		ENDIF

	FOR i= 1 TO MIN(errorI,VARDIM1(errorQ))
		IF STRLEN(errorQ[i])<75 THEN t=s/2 ELSE t=s
		UI_PICT "dk_attention 16x16.png", 0,48+(i-1)*s, 16,16, 1
		UI_OUTFIELD errorQ[i], 20,46+(i-1)*s+(s-t)*0.3, 424,t, 0+0  UI_TOOLTIP errorT[i]
		NEXT i
	RETURN									

