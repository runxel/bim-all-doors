"SELECT_dk_UCrelation":
	j = 0 : DIM  UIpicIdxArray[], UItextArray[], UIvaluesArray[]
	IF bs_door_leafs = 1 THEN
		j=j+1 : UIpicIdxArray[j] = 1 : UIvaluesArray[j] = 3  : UItextArray[j] = "Zarge <-> Beschlag bei ... 90°"
		j=j+1 : UIpicIdxArray[j] = 1 : UIvaluesArray[j] = 5  : UItextArray[j] = "... " + STR(MAX(UCminimalAngleGB, UCminimalAngleGS, UCminimalAngleGT),3,1) + "° = max. Durchgang"
		j=j+1 : UIpicIdxArray[j] = 1 : UIvaluesArray[j] = 9  : UItextArray[j] = "... " + STR(dk_open_leaf[dka_leaf_angle2D],1,0) + "° gemäß 2D"	
		j=j+1 : UIpicIdxArray[j] = 1 : UIvaluesArray[j] = 17 : UItextArray[j] = "... " + STR(dk_open_leaf[dka_leaf_angle3D],1,0) + "° gemäß 3D"	
		ENDIF
	IF bs_door_leafs = 2 THEN
		j=j+1 : UIpicIdxArray[j] = 5 : UIvaluesArray[j] = 3  : UItextArray[j] = "Beschlag <-> Beschlag je bei ... 90°"
		j=j+1 : UIpicIdxArray[j] = 5 : UIvaluesArray[j] = 5  : UItextArray[j] = "... " + STR(MAX(UCminimalAngleSB, UCminimalAngleST),3,1) + "° (SF) / " + STR(MAX(UCminimalAngleGB, UCminimalAngleGS, UCminimalAngleGT),3,1) + "° (GF) = max. Durchgang"
		j=j+1 : UIpicIdxArray[j] = 5 : UIvaluesArray[j] = 9  : UItextArray[j] = "... " + STR(dk_open_leaf[dka_leaf2_angle2D],1,0) + "° (SF) / " + STR(dk_open_leaf[dka_leaf_angle2D],1,0) + "° (GF) gemäß 2D"	
		j=j+1 : UIpicIdxArray[j] = 5 : UIvaluesArray[j] = 17 : UItextArray[j] = "... " + STR(dk_open_leaf[dka_leaf2_angle3D],1,0) + "° (SF) / " + STR(dk_open_leaf[dka_leaf_angle3D],1,0) + "° (GF) gemäß 3D"	
		ENDIF
	IF bs_door_leafs = 2 THEN
		j=j+1 : UIpicIdxArray[j] = 3 : UIvaluesArray[j] = 35 : UItextArray[j] = "Standflügel 0° <-> Beschlag bei ... 90°"
		j=j+1 : UIpicIdxArray[j] = 3 : UIvaluesArray[j] = 37 : UItextArray[j] = "... " + STR(MAX(UCminimalAngleGB, UCminimalAngleGS, UCminimalAngleGT),3,1) + "° (GF) = max. Durchgang"
		j=j+1 : UIpicIdxArray[j] = 3 : UIvaluesArray[j] = 41 : UItextArray[j] = "... " + STR(dk_open_leaf[dka_leaf_angle2D],1,0) + "° (GF) gemäß 2D"	
		j=j+1 : UIpicIdxArray[j] = 3 : UIvaluesArray[j] = 49 : UItextArray[j] = "... " + STR(dk_open_leaf[dka_leaf_angle3D],1,0) + "° (GF) gemäß 3D"	
		ENDIF

	IF bs_door_leafs = 1 AND bs_demand_Accessibility # 1 AND bs_demand_EscapeRoute # 1 THEN
		j=j+1 : UIpicIdxArray[j] = 2 : UIvaluesArray[j] = 2  : UItextArray[j] = "Zarge <-> Türblatt bei ... 90°"
		j=j+1 : UIpicIdxArray[j] = 2 : UIvaluesArray[j] = 4  : UItextArray[j] = "... " + STR(UCminimalAngleGT,3,1) + "° max. Durchgang"
		j=j+1 : UIpicIdxArray[j] = 2 : UIvaluesArray[j] = 8  : UItextArray[j] = "... " + STR(dk_open_leaf[dka_leaf_angle2D],1,0) + "° gemäß 2D"
		j=j+1 : UIpicIdxArray[j] = 2 : UIvaluesArray[j] = 16 : UItextArray[j] = "... " + STR(dk_open_leaf[dka_leaf_angle3D],1,0) + "° gemäß 3D"
		ENDIF
	IF bs_door_leafs = 2 AND bs_demand_Accessibility # 1 AND bs_demand_EscapeRoute # 1 THEN
		j=j+1 : UIpicIdxArray[j] = 6 : UIvaluesArray[j] = 2  : UItextArray[j] = "Türblatt <-> Türblatt je bei ... 90°"
		j=j+1 : UIpicIdxArray[j] = 6 : UIvaluesArray[j] = 4  : UItextArray[j] = "... " + STR(UCminimalAngleGT,3,1) + "° (SF/GF) = max. Durchgang"
		j=j+1 : UIpicIdxArray[j] = 6 : UIvaluesArray[j] = 8  : UItextArray[j] = "... " + STR(dk_open_leaf[dka_leaf2_angle2D],1,0) + "° (SF) / " + STR(dk_open_leaf[dka_leaf_angle2D],1,0) + "° (GF) gemäß 2D"
		j=j+1 : UIpicIdxArray[j] = 6 : UIvaluesArray[j] = 16 : UItextArray[j] = "... " + STR(dk_open_leaf[dka_leaf2_angle3D],1,0) + "° (SF) / " + STR(dk_open_leaf[dka_leaf_angle3D],1,0) + "° (GF) gemäß 3D"
		ENDIF
	IF bs_door_leafs = 2 AND bs_demand_Accessibility # 1 AND bs_demand_EscapeRoute # 1 THEN
		j=j+1 : UIpicIdxArray[j] = 4 : UIvaluesArray[j] = 34 : UItextArray[j] = "Standflügel 0° <-> Türblatt bei ... 90°"
		j=j+1 : UIpicIdxArray[j] = 4 : UIvaluesArray[j] = 36 : UItextArray[j] = "... " + STR(UCminimalAngleGT,3,1) + "° max. Durchgang"
		j=j+1 : UIpicIdxArray[j] = 4 : UIvaluesArray[j] = 40 : UItextArray[j] = "... " + STR(dk_open_leaf[dka_leaf_angle2D],1,0) + "° gemäß 2D"
		j=j+1 : UIpicIdxArray[j] = 4 : UIvaluesArray[j] = 48 : UItextArray[j] = "... " + STR(dk_open_leaf[dka_leaf_angle3D],1,0) + "° gemäß 3D"
		ENDIF
	RETURN

