!------------------------------------------------------------------- [ Einstellungen / settings ]

!»» Abmessungen / dimensions
	maxX=444    ! max. Breite/max. width
	maxY=265    ! Maximalhöhe /max. height
	LM=0        ! Linker Rand/Left margin
	RM=maxX-LM  ! Rechter Rand / right margin position
	uiTH=15     ! Texthöhe / height of text
	uiFH=20     ! Eingabefeldhöhe / height of input fields
	uiZH=uiFH+10! Zeilenhöhe / line height
	ys=4	    ! Versatz Text zu EIngabefeld
	uiY=0       ! vertical starting position

!»» Teilungen
	uiL=110         ! Breite Zeilenbeschriftung
	uiB=RM-LM       ! Breite Darstellungsfeld
	uiR=uiB-uiL     ! Breite Parameterfeld
	uiF=5           ! Eingabefeldabstand
	uiS=(uiR+uiF)/3 !Spaltenbreite der Eingabefelder nebeneinander

!»» Standardeinstellungen Zeile
	uiTH=16 : uiFH=20          ! Text- und Feldhöhe
	uiZH=uiFH+5 : uiZH2=uiTH+2 ! Zeilenhöhen
	ys=INT((uiFH-uiTH)/2)+1    ! Versatz SChriftlinie
	uiBB=16 : uiBH=16 : yb=(uiFH-uiBH)/2

!»» Standardeinstellungen Aufteilungen
	LM=0 : RM=maxX-LM  ! Linker/Rechter Rand / left/right margin position
	uiL=0 : uiF=5 : uiB=RM-LM : uiR=uiB-uiL : uiS=(uiR+uiF)/1


!------------------------------------------------------------------ [ Conditionale Voreinstellungen ]

FOR i=1 TO 3   !»» Level of Detail  ...
	IF i=1 THEN dki_LOD = 1
	IF i=2 THEN dki_LOD = 3
	IF i=3 THEN dki_LOD = 2
	IF dk_LOD[dki_LOD] = -1 THEN  !»» ... maßstabsabhängig
		IF GLOB_SCALE < 10 THEN
			dk_LOD[dki_LOD] = 500
			ELSE
			IF GLOB_SCALE < 26 THEN
				dk_LOD[dki_LOD] = 400
				ELSE
				IF GLOB_SCALE < 51 THEN
					dk_LOD[dki_LOD] = 350
					ELSE
					IF GLOB_SCALE < 101 THEN
						dk_LOD[dki_LOD] = 300
						ELSE
						IF GLOB_SCALE < 500 THEN
							dk_LOD[dki_LOD] = 200
							ELSE
							dk_LOD[dki_LOD] = 100
							ENDIF
						ENDIF
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	IF dk_LoD[dki_LOD] > 0 THEN dk_LoDtxt = dk_LoDtxt + STR(dk_LoD[dki_LOD],1,0) ELSE dk_LoDtxt = dk_LoDtxt + "[???]"
	IF i<3 THEN dk_LoDtxt = dk_LoDtxt + " | "
	NEXT i


!-------------------------------------------- [ Standardelemente aller Seiten ]

! #### Standardwerte

DIM UIpicIdxArray[], UItextArray[], UIvaluesArray[] ! Arrays für diskrete Auswahlen UI_INFIELD{4}
DIM UI_page_type[], UI_page_names[], UI_page_icons[]

UIpage = 0
UIpage_masterparent = -1


! #### Dialoginitialisierung

IF ABS(version - versionStamp) < tlr THEN txt = "" ELSE txt = " / v"+str("%^.2",versionStamp)
UI_DIALOG uiTXT[1] + " (v"+str("%^.2",version) + txt + " ©2021 dormakaba)"
UI_CURRENT_PAGE gs_ui_current_page


! =============================================================================
! Dialogaufrufe
! =============================================================================

IF show_start_screen > 0 THEN

	! Türobjekt wird gerade neu abgesetzt

	IF show_start_screen = 1 THEN GOSUB 9700 ELSE GOSUB 9800 ! Start screen / Splash

	ELSE

	! Regel-Dialoge

	GOSUB 1000 ! Common, basic settings
	if dk_LOI=4 then
		UIpage_masterparent = UIpage
		GOSUB 1400 ! demands
		UIpage_masterparent = -1
		ENDIF
	if dk_LOI>4 then
		UIpage_masterparent = UIpage
		GOSUB 1100 ! protection demands
		GOSUB 1200 ! dimension demands
		GOSUB 1300 ! functional demands
		UIpage_masterparent = -1
		ENDIF

	GOSUB 1600 ! Loaded components

	if dk_LOI>3 then ! Basic settings
		GOSUB 2000
		ELSE
		GOSUB 2100
		ENDIF

	GOSUB 500 ! Frame

	IF bs_door_leafs > 0 THEN
		GOSUB 550 ! Panels
		GOSUB 600 ! Hinges
		GOSUB 650 ! Door Automation
		GOSUB 700 ! Closer
		GOSUB 750 ! Case
		GOSUB 800 ! Access control
		GOSUB 850 ! Handles
		GOSUB 900 ! Grips / Pushbar
		ENDIF

	GOSUB 3000 ! representation
	UIpage_masterparent = UIpage

	GOSUB 3200 ! wallhole

	IF bs_door_leafs > 0 THEN
		GOSUB 3100 ! opening lines
		GOSUB 3300 ! minimal (usable) space
		ENDIF

	UIpage_masterparent = -1

	GOSUB 999  ! Infoseite

	ENDIF


