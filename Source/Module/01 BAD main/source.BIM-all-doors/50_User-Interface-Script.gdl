		ui_pict_AbmessungVU = "dk_dims_VZ 170x12.png"
		ENDIF
	ui_pict_AbmessungH = ui_pict_AbmessungH + "_"+txt
	ui_pict_AbmessungV = ui_pict_AbmessungV + "_"+txt

	IF gs_sidelight_left  THEN txt="L" ELSE txt = ""
	IF gs_sidelight_right THEN txt=txt+"R"
	ui_pict_AbmessungH = ui_pict_AbmessungH + "_H"+txt

	IF oberLichtBlende THEN txt="O" ELSE txt = ""
	IF oberLichtBlende = 2 THEN txt="B"
	ui_pict_AbmessungV = ui_pict_AbmessungV + "_V"+txt

	
	IF stumpf THEN txt = "S" ELSE txt="F"
	ui_pict_AbmessungH = ui_pict_AbmessungH+"_" + txt
	ui_pict_AbmessungV = ui_pict_AbmessungV+"_" + txt

	ui_pict_AbmessungH = ui_pict_AbmessungH+" 173x94.png"
	ui_pict_AbmessungV = ui_pict_AbmessungV+" 90x98.png"
	RETURN

"explainUsableClearance":
	txt = "" !  "Lichter Durchgang:\n"
	IF bs_door_leafs < 1 THEN

		txt = txt + "Keine Tür definiert. Lichter Durchgang wie Rohbaulichte"

		ELSE

		txt = txt + "Zwischen "

		IF bittest(dk_UCrelation, 0) THEN 
			IF bs_door_leafs = 1 THEN
				txt = txt + "Türgriffen und Zargeninnenkante "
				ELSE
				IF BITTEST(dk_UCrelation, 5) THEN
					txt = txt + "geschlossenem Standflügel und dem Türgriff (GF) "
					ELSE
					txt = txt + "den Türgriffen beider Flügel "
					ENDIF
				ENDIF
			ELSE 
			IF bs_door_leafs = 1 THEN
				txt = txt + "Türblatt (BGS) und Zargeninnenkante "
				ELSE
				txt = txt + "den Türblättern (BGS) beider Flügel "
				ENDIF
			ENDIF

		IF bs_door_leafs = 1 THEN

			IF BITTEST(dk_UCrelation, 1) THEN
				txt = txt + " bei 90°."
				ELSE
				IF BITTEST(dk_UCrelation, 2) THEN
					IF BITTEST(dk_UCrelation, 0) THEN
						txt = txt + "bei " + STR(UCminimalAngleGT,1,0) + "°."
						ELSE
						txt = txt + "bei " + STR(MAX(UCminimalAngleGB, UCminimalAngleGT),1,0) + "°."
						ENDIF
					ELSE
					IF BITTEST(dk_UCrelation, 3) THEN
						txt = txt + "bei " + STR(dk_open_leaf[dka_leaf_angle2D],1,0) + "°."
						ELSE
						IF BITTEST(dk_UCrelation, 4) THEN
							txt = txt + "bei " + STR(dk_open_leaf[dka_leaf_angle3D],1,0) + "°."
							ENDIF
						ENDIF
					ENDIF
				ENDIF

			ELSE

			txt = txt + "bei "

			! bs_door_leafs = 2 (Standflügel)
			IF BITTEST(dk_UCrelation, 5) THEN
				ELSE
				IF BITTEST(dk_UCrelation, 1) THEN
					txt = txt + "90° (SF) / "
					ELSE
					IF BITTEST(dk_UCrelation, 2) THEN
						IF BITTEST(dk_UCrelation, 0) THEN
							! txt = txt + STR(UCminimalAngleST,1,0) + "|"
							! txt = txt + STR(UCminimalAngleSB,1,0) + " "
							txt = txt + STR(MAX(UCminimalAngleSB, UCminimalAngleST),1,0) + "° (SF) / "
							ELSE
							! txt = txt + STR(UCminimalAngleST,1,0) + "|"
							! txt = txt + STR(UCminimalAngleSB,1,0) + " "
							txt = txt + STR(UCminimalAngleST,1,0) + "° (SF) / "
							ENDIF
						ELSE
						IF BITTEST(dk_UCrelation, 3) THEN
							txt = txt + STR(dk_open_leaf[dka_leaf2_angle2D],1,0) + "° (SF) / "
							ELSE
							IF BITTEST(dk_UCrelation, 4) THEN
								txt = txt + STR(dk_open_leaf[dka_leaf2_angle3D],1,0) + "° (SF) / "
								ENDIF
							ENDIF
						ENDIF
					ENDIF
				ENDIF

			! bs_door_leafs = 2 (Gangflügel)
			IF BITTEST(dk_UCrelation, 1) THEN
				txt = txt + "90"
				ELSE
				IF BITTEST(dk_UCrelation, 2) THEN
					IF BITTEST(dk_UCrelation, 0) THEN
						txt = txt + STR(MAX(UCminimalAngleGB, UCminimalAngleGT),1,0)
						ELSE
						txt = txt + STR(UCminimalAngleGT,1,0)
						ENDIF
					ELSE
					IF BITTEST(dk_UCrelation, 3) THEN
						txt = txt + STR(dk_open_leaf[dka_leaf_angle2D],1,0)
						ELSE
						IF BITTEST(dk_UCrelation, 4) THEN
							txt = txt + STR(dk_open_leaf[dka_leaf_angle3D],1,0)
							ENDIF
						ENDIF
					ENDIF
				ENDIF
			IF BITTEST(dk_UCrelation, 5) THEN
				txt = txt + "°. (Standflügel geschlossen)"
				ELSE
				txt = txt + "° (GF)."
				ENDIF
			ENDIF

		ENDIF

	RETURN

